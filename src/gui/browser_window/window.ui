<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="BrowserWindow" parent="GtkApplicationWindow">
    <property name="width-request">1200</property>
    <property name="title" translatable="yes">VulBr</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkSearchEntry" id="entry">
            <property name="placeholder-text" translatable="yes"></property>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="hscrollbar-policy">never</property>
            <property name="min-content-height">360</property>
            <property name="vexpand">true</property>
	    <child>
	      <object class="GtkBox" id="content_area" />
	    </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
